{
  "name": "@scalecube/scalecube-js",
  "version": "0.0.1",
  "description": "define API's cross convention,",
  "main": "App.js",
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "test:debug": "cross-env BABEL_ENV=jest babel tests src --out-dir debug --watch",
    "test": "cross-env BABEL_ENV=jest jest --runInBand --no-cache --env=jsdom",
    "test:snapshot": "cross-env BABEL_ENV=jest jest --maxWorkers=4 -u",
    "test:inspect": "cross-env BABEL_ENV=jest NODE_ENV=development node --debug-brk ./node_modules/jest/bin/jest.js --runInBand --no-cache",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "test:cov:report": "cat \"coverage/lcov.info\" | codacy-coverage",
    "build:es": "babel ./src --out-dir es",
    "build": "npm run build:es && npm run flow:gen",
    "postinstall": "flow-typed install",
    "flow": "flow",
    "flow:gen": "flow gen-flow-files ./src/scalecube-services/services/libdef.js --out-dir ./es/scalecube-services/services && npm run flow:gen:move",
    "flow:gen:move": "rname \"./es/scalecube-services/services/libdef.js.flow\" \"./es/scalecube-services/services/index.js.flow\"",
    "flowToTs": "node build/flowToTs es/scalecube-services/services/index.js.flow es/scalecube-services/services/index.d.ts",
    "flow:reset": "flow stop && flow start"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "cross-env": "^5.1.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.59.0",
    "flow-typed": "^2.2.3",
    "jest": "^21.2.1",
    "regenerator-runtime": "^0.11.0",
    "rename-cli": "^5.0.0"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js|jsx)$",
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "./"
    ]
  }
}
